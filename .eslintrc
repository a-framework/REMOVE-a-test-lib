{
  "extends": "airbnb/base",
  "rules": {
    "object-curly-spacing": [2, "never"],
    "arrow-body-style": 0,
    "indent": [2, 4],
    "comma-dangle": 0,
    "no-trailing-spaces": 0,
    "space-before-function-paren": 0,
    "func-names": 0,
    "react/prefer-stateless-function": 0,
    "new-cap": 0,
    "max-len": [2, 120]
  }
}
